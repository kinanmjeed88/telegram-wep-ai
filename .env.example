# Environment variables for TechTouch application
# Copy this file to .env.local and fill in your actual API keys

# Required API Keys
GEMINI_API_KEY=your_gemini_api_key_here
NETLIFY_API_KEY=your_netlify_api_key_here

# Application Configuration
NODE_ENV=development
VITE_APP_NAME=TechTouch
VITE_APP_VERSION=1.0.0

# API Configuration
API_RATE_LIMIT=100
API_TIMEOUT=30000

# Security Settings
CORS_ORIGIN=*
ENABLE_RATE_LIMITING=true

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_IMAGE_GENERATION=true
ENABLE_SEARCH=true

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=
MIXPANEL_TOKEN=

# Debug Settings
DEBUG_MODE=false
LOG_LEVEL=info